{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "build/index.js",
  "engines": {
    "node": ">= 16.13.0"
  },
  "scripts": {
    "start-client": "vite serve src/client --config vite.config.ts",
    "start-server": "tsx watch src/server/index.ts",
    "loadtest": "tsx loadtest/example.ts --room my_room --numClients 2",
    "build-server": "npm run clean && tsc",
    "clean": "rimraf build",
    "test": "mocha -r tsx test/**_test.ts --exit --timeout 15000"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.15.0",
    "@colyseus/testing": "^0.15.0",
    "@types/express": "^4.17.1",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "mocha": "^10.2.0",
    "postcss": "^8.4.48",
    "postcss-plugin": "^1.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.0",
    "tsx": "^4.10.2",
    "typescript": "^5.0.4",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.15.0",
    "@colyseus/playground": "^0.15.3",
    "@colyseus/tools": "^0.15.0",
    "colyseus": "^0.15.0",
    "colyseus.js": "^0.15.26",
    "easystarjs": "^0.4.4",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "mongodb": "6.10",
    "nanoid": "^5.0.8",
    "phaser": "^3.86.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1"
  }
}
